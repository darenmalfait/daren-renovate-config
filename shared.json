{
  "extends": [
    "config:best-practices",
    ":approveMajorUpdates",
    ":separateMultipleMajorReleases",
    ":semanticPrefixFixDepsChoreOthers",
    "schedule:daily"
  ],
  "ignorePresets": [":prHourlyLimit2"],
  "prConcurrentLimit": 3,
  "rangeStrategy": "bump",
